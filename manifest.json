{
  "manifest_version": 3,
  "name": "做JS银河",
  "version": "1.0",
  "description": "Automatically click elements based on specific keywords.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
